{
    "build": {
        "env": {
            "VITE_GAME_SERVER_URL": "@vite_game_server_url"
        }
    },
    "functions": {
        "src/pages/*.tsx": {
            "runtime": "@vercel/static"
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ],
    "env": {
        "VITE_GAME_SERVER_URL": "https://sui-battle-arena-production.up.railway.app"
    }
}